{
  "version": 2,
  "builds": [
    {
      "src": "flask_app/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/flask_app/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "flask_app/wsgi.py"
    }
  ]
} 